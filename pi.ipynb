   "source": [
    "from math import sqrt\n",
    "\n",
    "d = sqrt(2) # length of side of a square inscribed in a unit circle.\n",
    "\n",
    "length = 10\n",
    "g = [4 * 2 ** (n - 1) for n in range(1, length + 1)] # 4,8,16,32,64,...\n",
    "for i in g:\n",
    "    d2 = sqrt(2 - 2 * sqrt(1 - (d*d) / 4))\n",
    "    print((d*i)/2)\n",
    "    d = d2\n"
   ]
